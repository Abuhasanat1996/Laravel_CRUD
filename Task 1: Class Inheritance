<?php
class Circle {
    
    public function getArea($radius) {
        return pi() * pow($radius, 2);
    }
}

class Rectangle {
    public function getArea($width,$height) {
        return $width * $height;
    }
}

$circle = new Circle();
echo "The area of the circle is: " . $circle->getArea(3) . "\n";

$rectangle = new Rectangle();
echo "The area of the rectangle is: " . $rectangle->getArea(4,8);
?>
